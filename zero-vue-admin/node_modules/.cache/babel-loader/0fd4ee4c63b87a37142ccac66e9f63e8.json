CAC;QACA;QACA;MACA,CAJA;IAKA,CARA;IASAC,YATA,0BASA;MACA;MACA;IACA,CAZA;IAaAC,kBAbA,8BAaAC,GAbA,EAaA7B,MAbA,EAaA;MACA;QACAa,eADA;QAEAd;MAFA;MAIA8B;IACA,CAnBA;IAoBAC,UApBA,sBAoBA5C,IApBA,EAoBA;MACA;MAAA;;MACA;QACA;MACA;IACA,CAzBA;IA0BA6C,QA1BA,oBA0BAC,KA1BA,EA0BA;MACA;QACA;MACA,CAFA,MAEA;QACA;MACA;;MACA;IACA,CAjCA;IAkCAC,SAlCA,uBAkCA;MACA;QACArC,aADA;QAEAb,QAFA;QAGAmD,OAHA;QAIAC,SAJA;QAKAC,SALA;QAMAC,sBANA;QAOAxC;MAPA;IASA,CA5CA;IA6CAyC,YA7CA,0BA6CA;MAAA;;MACA;MACA;MACA;MACA;MACA;QACA;MACA,CAFA;IAGA,CArDA;IAsDAC,iBAtDA,6BAsDAV,GAtDA,EAsDA;MAAA;;MACA;MACA;MACA;MACA;MACA;QACA;MACA,CAFA;IAGA,CA9DA;IA+DAW,UA/DA,wBA+DA;MAAA;;MACA;QACA;UACA;UACAd;UACAe;YACA;YACA;;YACA;;YACA;cACA3C,WADA;cAEAe,eAFA;cAGAd,eAHA,CAIA;;YAJA;UAMA,CAVA;QAWA;MACA,CAhBA;IAiBA,CAjFA;IAkFA2C,YAlFA,wBAkFAb,GAlFA,EAkFA;MAAA;;MACA;MACA;MACA;MACA;QACA;MACA,CAFA;IAGA,CAzFA;IA0FAc,UA1FA,wBA0FA;MAAA;;MACA;QACA;UACA;UACAC;UACAA;UACAC;YAAA,2CACA,WADA;YAAA;;YAAA;cACA;gBAAA;;gBACA;kBACA;;kBACA;;kBACA;gBACA;cACA;YAPA;cAAA;YAAA;cAAA;YAAA;;YAQA;;YACA;;YACA;cACA/C,WADA;cAEAe,eAFA;cAGAd;YAHA;UAKA,CAfA;QAgBA;MACA,CAtBA;IAuBA,CAlHA;IAmHA+C,YAnHA,wBAmHAjB,GAnHA,EAmHA;MAAA;;MACA;QACA;UACAkB,eADA;UAEAlC,yBAFA;UAGAd;QAHA;QAKA;MACA;;MACA2B;MACA;QACAsB,uBADA;QAEAC,sBAFA;QAGAlD;MAHA,GAIAmD,IAJA,CAIA;QACAC;UACA;;UACA;YACApD,eADA;YAEAc;UAFA;QAIA,CANA;MAOA,CAZA,EAYAuC,KAZA,CAYA;QACA;UACArD,YADA;UAEAc;QAFA;MAIA,CAjBA;IAkBA;EA/IA;AAnDA","names":["name","directives","waves","data","tableKey","list","total","listLoading","listQuery","current","pageSize","createBy","temp","id","remarks","title","type","status","orderNum","parentId","dialogFormVisible","dialogStatus","textMap","update","create","dialogPvVisible","dialogRoleVisible","pvData","rules","required","message","trigger","timestamp","downloadLoading","options","value","userRole","userId","roleId","created","methods","getList","querDeptList","console","handleFilter","handleModifyStatus","row","sortChange","sortByID","order","resetTemp","age","phone","email","createTime","handleCreate","handleCreateChild","createData","createDept","handleUpdate","updateData","tempData","updateDept","handleDelete","showClose","confirmButtonText","cancelButtonText","then","deleteDept","catch"],"sourceRoot":"src/views/system/dept","sources":["index.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"filter-container\">\n      <el-input v-model=\"listQuery.name\" placeholder=\"机构名称\" style=\"width: 200px;\" class=\"filter-item\" @keyup.enter.native=\"handleFilter\"/>\n      <el-input v-model=\"listQuery.createBy\" placeholder=\"创建人\" style=\"width: 200px;\" class=\"filter-item\" @keyup.enter.native=\"handleFilter\"/>\n      <el-button v-waves class=\"filter-item\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleFilter\">\n        查询\n      </el-button>\n      <el-button v-waves class=\"filter-item\" style=\"margin-left: 10px;\" type=\"primary\" icon=\"el-icon-edit\" @click=\"handleCreate\">\n        添加\n      </el-button>\n    </div>\n\n    <el-table\n      v-loading=\"listLoading\"\n      :data=\"list\"\n      element-loading-text=\"Loading\"\n      row-key=\"id\"\n      fit\n      highlight-current-row\n      :tree-props=\"{children: 'children', hasChildren: 'hasChildren'}\">\n    >\n      <el-table-column align=\"center\" label=\"ID\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.id }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"机构名称\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.name }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"父id\" >\n        <template slot-scope=\"scope\">\n          <span>{{ scope.row.parentId }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"排序\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.orderNum }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"创建人\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.createBy }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"创建时间\" align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.createTime }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"更新人\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.lastUpdateBy }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"更新时间\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.lastUpdateTime }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" width=\"300\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"{row}\">\n          <el-button v-waves type=\"primary\" size=\"mini\" @click=\"handleCreateChild(row)\">\n            添加\n          </el-button>\n          <el-button v-waves type=\"primary\" size=\"mini\" @click=\"handleUpdate(row)\">\n            编辑\n          </el-button>\n          <el-button v-waves size=\"mini\" type=\"danger\" @click=\"handleDelete(row)\">\n            删除\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination v-show=\"total>0\" :total=\"total\" :page.sync=\"listQuery.current\" :limit.sync=\"listQuery.pageSize\" @pagination=\"getList\"/>\n\n    <el-dialog :title=\"textMap[dialogStatus]\" :visible.sync=\"dialogFormVisible\">\n      <el-form ref=\"dataForm\" :rules=\"rules\" :model=\"temp\" label-position=\"left\" label-width=\"70px\" style=\"width: 400px; margin-left:50px;\">\n        <el-form-item label=\"机构名称\" prop=\"name\">\n          <el-input v-model=\"temp.name\"/>\n        </el-form-item>\n        <el-form-item label=\"排序\" prop=\"orderNum\">\n          <el-input v-model=\"temp.orderNum\"/>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button @click=\"dialogFormVisible = false\">\n          取消\n        </el-button>\n        <el-button v-waves type=\"primary\" @click=\"dialogStatus==='create'?createData():updateData()\">\n          确认\n        </el-button>\n      </div>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n  import { createDept, deleteDept, querDeptList, updateDept } from '@/api/system/dept/dept'\n  import {tree} from \"@/utils/utils\";\n  import waves from '@/directive/waves'\n\n  export default {\n    name: 'ComplexTable',\n    directives: { waves },\n    data() {\n      return {\n        tableKey: 0,\n        list: [],\n        total: 0,\n        listLoading: true,\n        listQuery: {\n          current: 1,\n          pageSize: 10,\n          name: '',\n          createBy: '',\n        },\n        temp: {\n          id: undefined,\n          remarks: '',\n          title: '',\n          type: '',\n          status: 'published',\n          name: '',\n          orderNum: 0,\n          parentId: 0,\n        },\n        dialogFormVisible: false,\n        dialogStatus: '',\n        textMap: {\n          update: 'Edit',\n          create: 'Create'\n        },\n        dialogPvVisible: false,\n        dialogRoleVisible: false,\n        pvData: [],\n        rules: {\n          type: [{ required: true, message: 'type is required', trigger: 'change' }],\n          timestamp: [{ type: 'date', required: true, message: 'timestamp is required', trigger: 'change' }],\n          title: [{ required: true, message: 'title is required', trigger: 'blur' }]\n        },\n        downloadLoading: false,\n        options: [],\n        value: '',\n        userRole: {\n          userId: '',\n          roleId: ''\n        },\n      }\n    },\n    created() {\n      this.getList()\n    },\n    methods: {\n      getList() {\n        this.listLoading = true\n        querDeptList(this.listQuery).then(response => {\n          console.log(response.data,66666)\n          this.list = tree(response.data,0,'parentId')\n          this.listLoading = false\n        })\n      },\n      handleFilter() {\n        this.listQuery.current = 1\n        this.getList()\n      },\n      handleModifyStatus(row, status) {\n        this.$message({\n          message: '操作成功',\n          type: 'success'\n        })\n        row.status = status\n      },\n      sortChange(data) {\n        const { prop, order } = data\n        if (prop === 'id') {\n          this.sortByID(order)\n        }\n      },\n      sortByID(order) {\n        if (order === 'ascending') {\n          this.listQuery.sort = '+id'\n        } else {\n          this.listQuery.sort = '-id'\n        }\n        this.handleFilter()\n      },\n      resetTemp() {\n        this.temp = {\n          id: undefined,\n          name: '',\n          age: '',\n          phone: '',\n          email: '',\n          createTime: new Date(),\n          remarks: ''\n        }\n      },\n      handleCreate() {\n        this.resetTemp()\n        this.dialogStatus = 'create'\n        this.dialogFormVisible = true\n        this.temp.parentId=0\n        this.$nextTick(() => {\n          this.$refs['dataForm'].clearValidate()\n        })\n      },\n      handleCreateChild(row) {\n        this.resetTemp()\n        this.dialogStatus = 'create'\n        this.dialogFormVisible = true\n        this.temp.parentId=Number(row.id)\n        this.$nextTick(() => {\n          this.$refs['dataForm'].clearValidate()\n        })\n      },\n      createData() {\n        this.$refs['dataForm'].validate((valid) => {\n          if (valid) {\n            this.temp.orderNum=Number(this.temp.orderNum)\n            console.log(this.temp,321232)\n            createDept(this.temp).then(() => {\n              // this.list.unshift(this.temp)\n              this.getList()\n              this.dialogFormVisible = false\n              this.$notify({\n                title: '成功',\n                message: '创建成功',\n                type: 'success'\n                // duration: 2000\n              })\n            })\n          }\n        })\n      },\n      handleUpdate(row) {\n        this.temp = Object.assign({}, row)\n        this.dialogStatus = 'update'\n        this.dialogFormVisible = true\n        this.$nextTick(() => {\n          this.$refs['dataForm'].clearValidate()\n        })\n      },\n      updateData() {\n        this.$refs['dataForm'].validate((valid) => {\n          if (valid) {\n            const tempData = Object.assign({}, this.temp)\n            tempData.orderNum=Number(this.temp.orderNum)\n            tempData.timestamp = +new Date(tempData.timestamp)\n            updateDept(tempData).then(() => {\n              for (const v of this.list) {\n                if (v.id === this.temp.id) {\n                  const index = this.list.indexOf(v)\n                  this.list.splice(index, 1, this.temp)\n                  break\n                }\n              }\n              this.getList()\n              this.dialogFormVisible = false\n              this.$notify({\n                title: '成功',\n                message: '更新成功',\n                type: 'success'\n              })\n            })\n          }\n        })\n      },\n      handleDelete(row) {\n        if (row.children){\n          this.$message({\n            showClose: true,\n            message: '警告哦，包含子项不能直接删除',\n            type: 'warning'\n          });\n          return\n        }\n        console.log(row)\n        this.$confirm('此操作将永久删除该条数据, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'error'\n        }).then(() => {\n          deleteDept(row.id).then(response => {\n            this.getList()\n            this.$message({\n              type: 'success',\n              message: '删除成功!'\n            })\n          })\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: '已取消删除'\n          })\n        })\n      },\n\n    }\n  }\n</script>\n"]}]}
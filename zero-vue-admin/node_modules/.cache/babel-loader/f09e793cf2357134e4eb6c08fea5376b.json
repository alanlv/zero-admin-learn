A;IAcAC,kBAdA,8BAcAC,GAdA,EAcAC,MAdA,EAcA;MACA;QACAf,eADA;QAEAF;MAFA;MAIAgB;IACA,CApBA;IAsBAE,SAtBA,uBAsBA;MACA;QACA9B,aADA;QAEAf,QAFA;QAGA8C,OAHA;QAIAC,SAJA;QAKAC,SALA;QAMAC,sBANA;QAOA/B;MAPA;IASA,CAhCA;IAiCAgC,YAjCA,0BAiCA;MAAA;;MACA;MACA;MACA;MACA;QACA;MACA,CAFA;IAGA,CAxCA;IAyCAC,UAzCA,wBAyCA;MAAA;;MACA;QACA;UACAC;YACA;;YACA;;YACA;cACApB,WADA;cAEAH,eAFA;cAGAF;YAHA;UAKA,CARA;QASA;MACA,CAZA;IAaA,CAvDA;IAwDA0B,YAxDA,wBAwDAV,GAxDA,EAwDA;MAAA;;MACA;MACA;MACA;MACA;MACA;QACA;MACA,CAFA;IAGA,CAhEA;IAiEAW,UAjEA,wBAiEA;MAAA;;MACA;QACA;UACA;UACAC;YAAA,2CACA,WADA;YAAA;;YAAA;cACA;gBAAA;;gBACA;kBACA;;kBACA;;kBACA;gBACA;cACA;YAPA;cAAA;YAAA;cAAA;YAAA;;YAQA;;YACA;cACAvB,WADA;cAEAH,eAFA;cAGAF;YAHA;UAKA,CAdA;QAeA;MACA,CAnBA;IAoBA,CAtFA;IAuFA6B,YAvFA,wBAuFAb,GAvFA,EAuFA;MAAA;;MACA;QACAc,uBADA;QAEAC,sBAFA;QAGA/B;MAHA,GAIAgC,IAJA,CAIA;QACAC;UAAA7C;QAAA;UACA;;UACA;YACAY,eADA;YAEAE;UAFA;QAIA,CANA;MAOA,CAZA,EAYAgC,KAZA,CAYA;QACA;UACAlC,YADA;UAEAE;QAFA;MAIA,CAjBA;IAkBA;EA1GA;AA5CA","names":["name","components","Pagination","directives","waves","data","tableKey","list","total","listLoading","listQuery","current","pageSize","jobName","temp","id","delFlag","orderNum","remarks","dialogFormVisible","dialogStatus","textMap","update","create","dialogPvVisible","dialogRoleVisible","rules","type","required","message","trigger","timestamp","title","downloadLoading","options","value","created","methods","getList","queryJobList","console","handleFilter","handleModifyStatus","row","status","resetTemp","age","phone","email","createTime","handleCreate","createData","createJob","handleUpdate","updateData","updateJob","handleDelete","confirmButtonText","cancelButtonText","then","deleteJob","catch"],"sourceRoot":"src/views/system/job","sources":["index.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"filter-container\">\n      <el-input v-model=\"listQuery.jobName\" placeholder=\"职位名称\" style=\"width: 200px;\" class=\"filter-item\" @keyup.enter.native=\"handleFilter\"/>\n      <el-button v-waves class=\"filter-item\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleFilter\">\n        查询\n      </el-button>\n      <el-button v-waves class=\"filter-item\" style=\"margin-left: 10px;\" type=\"primary\" icon=\"el-icon-edit\" @click=\"handleCreate\">\n        添加\n      </el-button>\n    </div>\n\n    <el-table\n      v-loading=\"listLoading\"\n      :data=\"list\"\n      element-loading-text=\"Loading\"\n      fit\n      highlight-current-row\n    >\n      <el-table-column align=\"center\" label=\"ID\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.id }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"职位名称\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.jobName }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"排序\" >\n        <template slot-scope=\"scope\">\n          <span>{{ scope.row.orderNum }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"状态\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.delFlag }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"备注\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.remarks }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"创建人\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.createBy }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"创建时间\" align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.createTime }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"更新人\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.lastUpdateBy }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"更新时间\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.lastUpdateTime }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" width=\"200\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"{row}\">\n          <el-button v-waves type=\"primary\" size=\"mini\"  @click=\"handleUpdate(row)\">\n            编辑\n          </el-button>\n          <el-button v-waves size=\"mini\" type=\"danger\"  @click=\"handleDelete(row)\">\n            删除\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination v-show=\"total>0\" :total=\"total\" :page.sync=\"listQuery.current\" :limit.sync=\"listQuery.pageSize\" @pagination=\"getList\"/>\n\n    <el-dialog :title=\"textMap[dialogStatus]\" :visible.sync=\"dialogFormVisible\">\n      <el-form ref=\"dataForm\" :rules=\"rules\" :model=\"temp\" label-position=\"left\" label-width=\"70px\" style=\"width: 400px; margin-left:50px;\">\n        <el-form-item label=\"职位名称\" prop=\"jobName\">\n          <el-input v-model=\"temp.jobName\"/>\n        </el-form-item>\n        <el-form-item label=\"排序\" prop=\"orderNum\">\n          <el-input v-model=\"temp.orderNum\"/>\n        </el-form-item>\n        <el-form-item label=\"状态\" prop=\"delFlag\">\n          <el-input v-model=\"temp.delFlag\"/>\n        </el-form-item>\n        <el-form-item label=\"备注\">\n          <el-input v-model=\"temp.remarks\" :autosize=\"{ minRows: 2, maxRows: 4}\" type=\"textarea\" placeholder=\"Please input\"/>\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button v-waves @click=\"dialogFormVisible = false\">\n          取消\n        </el-button>\n        <el-button v-waves type=\"primary\" @click=\"dialogStatus==='create'?createData():updateData()\">\n          确认\n        </el-button>\n      </div>\n    </el-dialog>\n\n  </div>\n</template>\n\n<script>\n  import { createJob, deleteJob, queryJobList, updateJob } from '@/api/system/job/job'\n  import waves from '@/directive/waves'\n  import Pagination from '@/components/Pagination'\n\n  export default {\n    name: 'ComplexTable',\n    components: { Pagination },\n    directives: { waves },\n    data() {\n      return {\n        tableKey: 0,\n        list: null,\n        total: 0,\n        listLoading: true,\n        listQuery: {\n          current: 1,\n          pageSize: 10,\n          jobName: '',\n        },\n        temp: {\n          id: undefined,\n          delFlag: 0,\n          jobName: \"\",\n          orderNum: 0,\n          remarks: \"\",\n\n        },\n        dialogFormVisible: false,\n        dialogStatus: '',\n        textMap: {\n          update: 'Edit',\n          create: 'Create'\n        },\n        dialogPvVisible: false,\n        dialogRoleVisible: false,\n        rules: {\n          type: [{ required: true, message: 'type is required', trigger: 'change' }],\n          timestamp: [{ type: 'date', required: true, message: 'timestamp is required', trigger: 'change' }],\n          title: [{ required: true, message: 'title is required', trigger: 'blur' }]\n        },\n        downloadLoading: false,\n        options: [],\n        value: '',\n      }\n    },\n    created() {\n      this.getList()\n    },\n    methods: {\n      getList() {\n        this.listLoading = true\n        queryJobList(this.listQuery).then(response => {\n          console.log(response.data)\n          this.list = response.data\n          this.total = response.total\n          this.listLoading = false\n        })\n      },\n      handleFilter() {\n        this.listQuery.current = 1\n        this.getList()\n      },\n      handleModifyStatus(row, status) {\n        this.$message({\n          message: '操作成功',\n          type: 'success'\n        })\n        row.status = status\n      },\n\n      resetTemp() {\n        this.temp = {\n          id: undefined,\n          name: '',\n          age: '',\n          phone: '',\n          email: '',\n          createTime: new Date(),\n          remarks: ''\n        }\n      },\n      handleCreate() {\n        this.resetTemp()\n        this.dialogStatus = 'create'\n        this.dialogFormVisible = true\n        this.$nextTick(() => {\n          this.$refs['dataForm'].clearValidate()\n        })\n      },\n      createData() {\n        this.$refs['dataForm'].validate((valid) => {\n          if (valid) {\n            createJob(this.temp).then(() => {\n              this.getList()\n              this.dialogFormVisible = false\n              this.$notify({\n                title: '成功',\n                message: '创建成功',\n                type: 'success'\n              })\n            })\n          }\n        })\n      },\n      handleUpdate(row) {\n        this.temp = Object.assign({}, row)\n        this.temp.timestamp = new Date(this.temp.timestamp)\n        this.dialogStatus = 'update'\n        this.dialogFormVisible = true\n        this.$nextTick(() => {\n          this.$refs['dataForm'].clearValidate()\n        })\n      },\n      updateData() {\n        this.$refs['dataForm'].validate((valid) => {\n          if (valid) {\n            const tempData = Object.assign({}, this.temp)\n            updateJob(tempData).then(() => {\n              for (const v of this.list) {\n                if (v.id === this.temp.id) {\n                  const index = this.list.indexOf(v)\n                  this.list.splice(index, 1, this.temp)\n                  break\n                }\n              }\n              this.dialogFormVisible = false\n              this.$notify({\n                title: '成功',\n                message: '更新成功',\n                type: 'success'\n              })\n            })\n          }\n        })\n      },\n      handleDelete(row) {\n        this.$confirm('此操作将永久删除该条数据, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'error'\n        }).then(() => {\n          deleteJob({id:row.id}).then(response => {\n            this.getList()\n            this.$message({\n              type: 'success',\n              message: '删除成功!'\n            })\n          })\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: '已取消删除'\n          })\n        })\n      },\n\n\n    }\n  }\n</script>\n"]}]}
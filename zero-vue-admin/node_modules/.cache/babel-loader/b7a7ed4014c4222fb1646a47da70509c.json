;QAIA,CANA;MAOA,CAZA,EAYAY,KAZA,CAYA;QACA;UACAX,YADA;UAEAD;QAFA;MAIA,CAjBA;IAkBA,CAnGA;IAqGAa,SArGA,uBAqGA;MACA;QACAC,aADA;QAEAC,QAFA;QAGAC,OAHA;QAIAC,SAJA;QAKAC,SALA;QAMAC,sBANA;QAOAC,WAPA;QAQAC;MARA;IAUA;EAhHA,CAFA;EAoHAC;IACA;EACA,CAtHA;EAwHAC,IAxHA,kBAwHA;IACA;MACAC,aADA;MAEAC;QACAX,aADA;QAEAY,aAFA;QAGAN,WAHA;QAIAO,qBAJA;QAKA5B,SALA;QAMAE,OANA;QAOAc,QAPA;QAQAa,mBARA;QASAP,OATA;QAUAQ,WAVA;QAWAC,gBAXA;QAYAC,WAZA;QAaAC,eAbA;QAcAC,WAdA;QAeAC;MAfA,CAFA;MAmBAb,OAnBA;MAoBAc;QACAC,cADA;QAEAC;MAFA,CApBA;MAwBAC,gBAxBA;MAyBAC,wBAzBA;MA0BAC;QACAC,UADA;QAEAC;MAFA;IA1BA;EAgCA;AAzJA","names":["directives","waves","methods","getList","queryMenuList","handleCreateWithOutPid","handleCreate","row","console","createData","createResources","title","message","type","handleUpdate","updateData","tempData","updateResources","handleDelete","showClose","confirmButtonText","cancelButtonText","then","deleteResources","catch","resetTemp","id","name","age","phone","email","createTime","remarks","pid","created","data","tableData","temp","importance","timestamp","status","vuePath","vueComponent","vueIcon","vueRedirect","parentId","orderNum","textMap","update","create","dialogStatus","dialogFormVisible","listQuery","current","pageSize"],"sourceRoot":"src/views/system/resource","sources":["index.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"filter-container\">\n      <el-input v-model=\"listQuery.name\" placeholder=\"权限名称\" style=\"width: 200px;\" class=\"filter-item\" @keyup.enter.native=\"\" />\n      <el-input v-model=\"listQuery.createBy\" placeholder=\"路径\" style=\"width: 200px;\" class=\"filter-item\" @keyup.enter.native=\"\" />\n      <el-button v-waves class=\"filter-item\" type=\"primary\" icon=\"el-icon-search\" @click=\"\">\n        查询\n      </el-button>\n      <el-button v-waves class=\"filter-item\" style=\"margin-left: 10px;\" type=\"primary\" icon=\"el-icon-edit\" @click=\"handleCreateWithOutPid\">\n        添加\n      </el-button>\n    </div>\n\n    <el-table\n      :data=\"tableData\"\n      style=\"width: 100%;margin-bottom: 20px;\"\n      row-key=\"id\"\n      highlight-current-row\n      :tree-props=\"{children: 'children', hasChildren: 'hasChildren'}\">\n      <el-table-column\n        prop=\"id\"\n        label=\"Id\"\n      >\n      </el-table-column>\n      <el-table-column\n        prop=\"name\"\n        label=\"权限名称\"\n       >\n      </el-table-column>\n      <el-table-column label=\"父id\" >\n        <template slot-scope=\"scope\">\n          <span>{{ scope.row.parentId }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"路径\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.vuePath }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"页面\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.vueComponent }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"重定向\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.vueRedirect }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"类型\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.type }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"图标\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.vueIcon }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"排序\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.orderNum }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"创建人\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.createBy }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"创建人\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.createBy }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"创建时间\" align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.createTime }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"更新人\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.lastUpdateBy }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"更新时间\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.lastUpdateTime }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" width=\"300\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"{row}\">\n          <el-button v-waves type=\"primary\" size=\"mini\" @click=\"handleCreate(row)\">\n            添加\n          </el-button>\n          <el-button v-waves type=\"primary\" size=\"mini\" @click=\"handleUpdate(row)\">\n            编辑\n          </el-button>\n\n          <el-button v-waves size=\"mini\" type=\"danger\" @click=\"handleDelete(row)\">\n            删除\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <el-dialog :title=\"textMap[dialogStatus]\" :visible.sync=\"dialogFormVisible\">\n      <el-form ref=\"dataForm\" :model=\"temp\" label-position=\"left\" label-width=\"70px\" style=\"width: 400px; margin-left:50px;\">\n        <el-form-item label=\"权限名称\" prop=\"name\">\n          <el-input v-model=\"temp.name\"/>\n        </el-form-item>\n        <el-form-item label=\"路径\" prop=\"vuePath\">\n          <el-input v-model=\"temp.vuePath\" />\n        </el-form-item>\n        <el-form-item label=\"页面\" prop=\"vueComponent\">\n          <el-input v-model=\"temp.vueComponent\" />\n        </el-form-item>\n        <el-form-item label=\"重定向\" prop=\"vueRedirect\">\n          <el-input v-model=\"temp.vueRedirect\" />\n        </el-form-item>\n        <el-form-item label=\"图标\" prop=\"vueIcon\">\n          <el-input v-model=\"temp.vueIcon\" />\n        </el-form-item>\n        <el-form-item label=\"类型\" prop=\"type\">\n          <el-input v-model=\"temp.type\"/>\n        </el-form-item>\n        <el-form-item label=\"排序\" prop=\"orderNum\">\n          <el-input v-model=\"temp.orderNum\" />\n        </el-form-item>\n      </el-form>\n      <div slot=\"footer\" class=\"dialog-footer\">\n        <el-button v-waves @click=\"dialogFormVisible = false\">\n          取消\n        </el-button>\n        <el-button v-waves type=\"primary\" @click=\"dialogStatus==='create'?createData():updateData()\">\n          确认\n        </el-button>\n      </div>\n    </el-dialog>\n  </div>\n\n</template>\n\n<script>\n  import { queryMenuList, createResources, deleteResources, updateResources } from '@/api/system/resource/resources'\n  import {tree} from \"@/utils/utils\";\n  import waves from '@/directive/waves'\n\n  export default {\n    directives: { waves },\n    methods: {\n      getList() {\n        queryMenuList(this.listQuery).then(response => {\n          this.tableData = tree(response.data,0,'parentId')\n        })\n      },\n      handleCreateWithOutPid() {\n        this.resetTemp()\n        this.dialogStatus = 'create'\n        this.dialogFormVisible = true\n        this.temp.parentId=0\n        this.$nextTick(() => {\n          this.$refs['dataForm'].clearValidate()\n        })\n      },\n      handleCreate(row) {\n        this.resetTemp()\n        this.temp.pid=row.id\n        console.log(this.temp.pid)\n        this.temp.parentId=Number(row.id)\n        this.dialogStatus = 'create'\n        this.dialogFormVisible = true\n        this.$nextTick(() => {\n          this.$refs['dataForm'].clearValidate()\n        })\n      },\n      createData() {\n        this.$refs['dataForm'].validate((valid) => {\n          if (valid) {\n            this.temp.orderNum=Number(this.temp.orderNum)\n            this.temp.type=Number(this.temp.type)\n            createResources(this.temp).then(() => {\n              this.dialogFormVisible = false\n              this.getList()\n              this.$notify({\n                title: '成功',\n                message: '创建成功',\n                type: 'success'\n              })\n            })\n          }\n        })\n      },\n      handleUpdate(row) {\n        this.temp = Object.assign({}, row)\n        this.dialogStatus = 'update'\n        this.dialogFormVisible = true\n\n        this.$nextTick(() => {\n          this.$refs['dataForm'].clearValidate()\n        })\n      },\n      updateData() {\n        this.$refs['dataForm'].validate((valid) => {\n          if (valid) {\n            const tempData = Object.assign({}, this.temp)\n            tempData.timestamp = +new Date(tempData.timestamp)\n            updateResources(tempData).then(() => {\n              this.getList()\n              this.dialogFormVisible = false\n              this.$notify({\n                title: '成功',\n                message: '更新成功',\n                type: 'success'\n              })\n            })\n          }\n        })\n      },\n      handleDelete(row) {\n        console.log(row.children)\n        if (row.children){\n          this.$message({\n            showClose: true,\n            message: '警告哦，包含子项不能直接删除',\n            type: 'warning'\n          });\n          return\n        }\n        console.log(row)\n\n        this.$confirm('此操作将永久删除该条数据, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'error'\n        }).then(() => {\n          deleteResources(row.id).then(response => {\n            this.getList()\n            this.$message({\n              type: 'success',\n              message: '删除成功!'\n            })\n          })\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: '已取消删除'\n          })\n        })\n      },\n\n      resetTemp() {\n        this.temp = {\n          id: undefined,\n          name: '',\n          age: '',\n          phone: '',\n          email: '',\n          createTime: new Date(),\n          remarks: '',\n          pid: ''\n        }\n      },\n    },\n    created: function() {\n      this.getList()\n    },\n\n    data() {\n      return {\n        tableData: [],\n        temp: {\n          id: undefined,\n          importance: 1,\n          remarks: '',\n          timestamp: new Date(),\n          title: '',\n          type: 0,\n          name: '',\n          status: 'published',\n          pid: '',\n          vuePath: '',\n          vueComponent: '',\n          vueIcon: '',\n          vueRedirect: '',\n          parentId: 0,\n          orderNum: 0,\n        },\n        pid: '',\n        textMap: {\n          update: 'Edit',\n          create: 'Create'\n        },\n        dialogStatus: '',\n        dialogFormVisible: false,\n        listQuery: {\n          current: 1,\n          pageSize: 10000\n\n        }\n      }\n    }\n  }\n</script>\n"]}]}
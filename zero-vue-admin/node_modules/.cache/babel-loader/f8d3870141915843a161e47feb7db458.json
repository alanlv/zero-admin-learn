pageSize","created","methods","getList","querySysLogList","handleFilter","handleDelete","row","confirmButtonText","cancelButtonText","type","then","deleteSysLogin","id","message","catch"],"sourceRoot":"src/views/log/syslog","sources":["index.vue"],"sourcesContent":["<template>\n  <div class=\"app-container\">\n    <div class=\"filter-container\">\n      <el-input v-model=\"listQuery.name\" placeholder=\"用户名\" style=\"width: 200px;\" class=\"filter-item\" @keyup.enter.native=\"handleFilter\"/>\n      <el-button v-waves class=\"filter-item\" type=\"primary\" icon=\"el-icon-search\" @click=\"handleFilter\">\n        查询\n      </el-button>\n    </div>\n\n    <el-table\n      v-loading=\"listLoading\"\n      :data=\"list\"\n      element-loading-text=\"Loading\"\n      fit\n      highlight-current-row\n    >\n      <el-table-column align=\"center\" label=\"ID\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.id }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"用户名\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.userName }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"用户操作\" >\n        <template slot-scope=\"scope\">\n          <span>{{ scope.row.operation }}</span>\n        </template>\n      </el-table-column>\n      <el-table-column label=\"请求方法\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.method }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"执行时间(毫秒)\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.time }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"IP地址\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.ip }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"创建时间\" align=\"center\">\n        <template slot-scope=\"scope\">\n          {{ scope.row.createTime }}\n        </template>\n      </el-table-column>\n      <el-table-column label=\"操作\" align=\"center\" width=\"100\" class-name=\"small-padding fixed-width\">\n        <template slot-scope=\"{row}\">\n          <el-button v-waves size=\"mini\" type=\"danger\" @click=\"handleDelete(row)\">\n            删除\n          </el-button>\n        </template>\n      </el-table-column>\n    </el-table>\n\n    <pagination v-show=\"total>0\" :total=\"total\" :page.sync=\"listQuery.current\" :limit.sync=\"listQuery.pageSize\" @pagination=\"getList\"/>\n\n  </div>\n</template>\n\n<script>\n  import { deleteSysLogin, querySysLogList} from '@/api/log/syslog'\n  import waves from '@/directive/waves'\n  import Pagination from '@/components/Pagination'\n\n  export default {\n    name: 'ComplexTable',\n    components: { Pagination },\n    directives: { waves },\n\n    data() {\n      return {\n        tableKey: 0,\n        list: null,\n        total: 0,\n        listLoading: true,\n        listQuery: {\n          current: 1,\n          pageSize: 10,\n          name: '',\n        },\n      }\n    },\n    created() {\n      this.getList()\n    },\n    methods: {\n      getList() {\n        this.listLoading = true\n        querySysLogList(this.listQuery).then(response => {\n          this.list = response.data\n          this.total = response.total\n          this.listLoading = false\n        })\n      },\n      handleFilter() {\n        this.listQuery.current = 1\n        this.getList()\n      },\n\n      handleDelete(row) {\n        this.$confirm('此操作将永久删除该条数据, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'error'\n        }).then(() => {\n          deleteSysLogin({id:row.id}).then(response => {\n            this.getList()\n            this.$message({\n              type: 'success',\n              message: '删除成功!'\n            })\n          })\n        }).catch(() => {\n          this.$message({\n            type: 'info',\n            message: '已取消删除'\n          })\n        })\n      },\n\n    }\n  }\n</script>\n"]}]}